source 'https://github.com/CocoaPods/Specs.git'

target "DefaultApp" do
  platform :osx, "10.14"
  inhibit_all_warnings!
  pod 'AppCenter'
  pod 'SwiftyJSON'
  pod 'Sparkle'
  pod 'Alamofire'
end

target "DefaultApp-MAS" do
  platform :osx, "10.14"
  inhibit_all_warnings!
  pod 'AppCenter'
  pod 'SwiftyJSON'
  pod 'Alamofire'
end
